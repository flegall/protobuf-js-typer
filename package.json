{
  "name": "protobuf-js-typer",
  "version": "0.0.1",
  "repository": "git@github.com:flegall/protobuf-js-typer.git",
  "description": "A Flowtype/typescript definitions generator for your .proto IDL files",
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js",
    "flow": "flow",
    "pretest": "npm run flow",
    "test": "ava src/**.spec.js -v"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "0.16.0",
    "chai": "3.5.0",
    "flow-bin": "0.33.0",
    "sinon": "1.17.6",
    "sinon-chai": "2.8.0"
  },
  "dependencies": {
    "babel-core": "6.16.0",
    "babel-plugin-transform-flow-strip-types": "6.14.0",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-polyfill": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "pegjs": "0.10.0",
    "yargs": "6.4.0"
  },
  "engines": {
    "node": ">=4"
  },
  "ava": {
    "require": [
      "babel-core/register",
      "./src/test-support/setup-sinon-chai.js"
    ],
    "babel": "inherit"
  }
}

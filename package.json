    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "babel-core": "^6.11.4",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2017": "^1.6.1",
    "pegjs": "^0.9.0",
    "yargs": "^4.8.1"
  },
  "engines": {
    "node": ">=4"
  },
  "ava": {
